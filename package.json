{
  "name": "browser-template",
  "version": "0.0.1",
  "description": "${yourDescription}",
  "main": "docs/index.html",
  "scripts": {
    "webpack": "./node_modules/.bin/webpack",
    "build": "npm-run-all build:*",
    "build:pug": "./node_modules/.bin/pug --hierarchy -o docs/ src/",
    "build:scss": "node-sass src/index.scss docs/index.css && postcss docs/index.css -u autoprefixer --no-map -o docs/index.css",
    "build:js": "npm run webpack",
    "watch": "npm-run-all --parallel watch:*",
    "watch:scss": "node-sass -w src/index.scss docs/index.css && postcss docs/index.css -u autoprefixer --no-map -o docs/index.css",
    "watch:pug": "npm run build:pug -- -w",
    "watch:js": "./node_modules/.bin/webpack-dev-server --config watch.webpack.config.js"
  },
  "keywords": [],
  "author": "Hiroki Usuba <hrky0206@gmail.com> (http://mimorisuzuko.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "postcss-cli": "^3.0.0",
    "pug-cli": "^1.0.0-alpha6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {}
}
