{
  "name": "browser-template",
  "version": "0.0.1",
  "description": "${yourDescription}",
  "main": "docs/index.html",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:pug": "./node_modules/.bin/pug --hierarchy -o docs/ src/",
    "build:js": "node fuse.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:pug": "npm run build:pug -- -w",
    "watch:js": "node fuse.js watch"
  },
  "keywords": [],
  "author": "Hiroki Usuba <hrky0206@gmail.com> (http://mimorisuzuko.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "fuse-box": "^2.1.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss": "^6.0.2",
    "pug-cli": "^1.0.0-alpha6"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
