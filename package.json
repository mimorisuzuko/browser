    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "pug-cli": "^1.0.0-alpha6",
    "react-hot-loader": "^1.3.1",
