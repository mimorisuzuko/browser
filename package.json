{
  "name": "browser-template",
  "version": "0.0.1",
  "description": "${yourDescription}",
  "main": "${yourJavaScriptFile}.js",
  "scripts": {
    "webpack": "./node_modules/.bin/webpack",
    "build": "npm run build:pug && npm run build:scss && ./node_modules/.bin/webpack",
    "build:pug": "./node_modules/.bin/pug --hierarchy -o docs/ src/",
    "build:scss": "./node_modules/node-sass/bin/node-sass src/index.scss docs/index.css --output-style compressed",
    "build:js": "./node_modules/.bin/webpack",
    "build:dev": "npm run build:pug && npm run build:scss && npm run build:js -- -d",
    "webpack-dev-server": "./node_modules/.bin/webpack-dev-server --content-base docs/",
    "watch:pug": "npm run build:pug -- -w",
    "watch:scss": "npm run build:scss -- -w",
    "watch:js": "npm run build:js -- -w"
  },
  "keywords": [],
  "author": "Hiroki Usuba <hrky0206@gmail.com> (http://mimorisuzuko.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "node-sass": "^4.5.0",
    "pug-cli": "^1.0.0-alpha6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
